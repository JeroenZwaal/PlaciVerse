  "components": {
    "schemas": {
      "Environment2D": {
        "required": [
          "maxHeight",
          "maxWidth",
          "name",
          "userId"
        ],
        "type": "object",
        "properties": {
          "environmentId": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "maxWidth": {
            "type": "integer",
            "format": "int32"
          },
          "maxHeight": {
            "type": "integer",
            "format": "int32"
          },
          "objects": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Object2D"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Object2D": {
        "required": [
          "environmentId",
          "positionX",
          "positionY",
          "prefabId",
          "rotationZ",
          "scaleX",
          "scaleY",
          "sortingLayer"
        ],
        "type": "object",
        "properties": {
          "objectId": {
            "type": "integer",
            "format": "int32"
          },
          "environmentId": {
            "type": "integer",
            "format": "int32"
          },
          "prefabId": {
            "type": "integer",
            "format": "int32"
          },
          "positionX": {
            "type": "number",
            "format": "double"
          },
          "positionY": {
            "type": "number",
            "format": "double"
          },
          "scaleX": {
            "type": "number",
            "format": "double"
          },
          "scaleY": {
            "type": "number",
            "format": "double"
          },
          "rotationZ": {
            "type": "number",
            "format": "double"
          },
          "sortingLayer": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "User": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "username": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UserLoginDto": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UserRegisterDto": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 10,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[\\W_]).+$",
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
